# Supabase - Copy from your Supabase project settings
NEXT_PUBLIC_SUPABASE_URL=https://your-project-ref.supabase.co
NEXT_PUBLIC_SUPABASE_ANON_KEY=your_anon_key_here

# ElevenLabs Agent Config (from the existing HubSpot widget source)
NEXT_PUBLIC_ELEVENLABS_AGENT_ID=your_elevenlabs_agent_id
NEXT_PUBLIC_ELEVENLABS_CONVERSATION_SIGNATURE=your_elevenlabs_conversation_signature

# HubSpot — server-side only (never expose to client)
HUBSPOT_PRIVATE_APP_TOKEN=pat-na1-xxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxx
HUBSPOT_VIDEO_ID=your_hubspot_file_id
# Custom behavioral event name — format: pe{hubId}_{internalName}
# Create under HubSpot > Data Management > Event Management (requires Marketing Hub Professional+)
# Add a numeric property named "watch_duration_seconds" to the event
HUBSPOT_VIDEO_EVENT_NAME=pe0000000_training_video_watched

# App URL (used for auth redirects)
# Local dev: http://localhost:3000
# Production: https://your-site.netlify.app
NEXT_PUBLIC_APP_URL=https://your-site.netlify.app

# n8n Webhook URLs (your n8n instance URL + path)
NEXT_PUBLIC_N8N_INVITE_WEBHOOK=https://your-n8n-instance.com/webhook/echo-barrier-user-invite
NEXT_PUBLIC_N8N_PROFILE_WEBHOOK=https://your-n8n-instance.com/webhook/echo-barrier-profile-completion
